{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "lupin",
      "camelcase": "Lupin",
      "title": "Lupin",
      "scope": "source.lupin",
      "file-types": [
        "lupin"
      ],
      "injection-regex": "^lupin$",
      "class-name": "TreeSitterLupin"
    }
  ],
  "metadata": {
    "version": "0.0.1",
    "license": "MIT",
    "description": "Lupin Tree Sitter",
    "authors": [
      {
        "name": "Iris Junckes",
        "email": "me@junckes.dev",
        "url": "https://junckes.dev/"
      }
    ],
    "links": {
      "repository": "https://github.com/j-junckes/tree-sitter-lupin"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}